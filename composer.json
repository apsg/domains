{
    "name": "apsg/domains",
    "description": "Setup helper for DDD approach in Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Szymon Gackowski",
            "email": "szymon@gackowski.edu.pl",
            "homepage": "http://gackowski.edu.pl"
        }
    ],
    "homepage": "https://github.com/apsg/domains",
    "keywords": ["Laravel", "Domains", "DDD"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Apsg\\Domains\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apsg\\Domains\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Apsg\\Domains\\DomainsServiceProvider"
            ],
            "aliases": {
                "Domains": "Apsg\\Domains\\Facades\\Domains"
            }
        }
    }
}
